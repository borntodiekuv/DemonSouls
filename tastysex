for i,v in next, game.CoreGui:GetChildren() do
    if v.Name == "ScreenGui" and v.ImageButton then
        v:Destroy()
    end
end

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/sannin9000/Ui-Libraries/main/uwuware", true))()

local Window = Library:CreateWindow"Demon Souls"



Window:AddToggle({text = "Auto Attack", flag = "toggle", state = false, callback = function(b)
    bb = b
    while bb do task.wait(.2)
        game:GetService("ReplicatedStorage").RemoteEvents.GeneralAttack:FireServer()
    end
end})

Window:AddToggle({text = "Collect Souls", flag = "toggle", state = false, callback = function(a)
    aa = a
    while aa do task.wait()
        pcall(function()
            for _,v in pairs(game:GetService("Workspace"):GetDescendants()) do
                if v:IsA("TouchTransmitter") and v.Parent.Name == "SoulTouch" then
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 0)
                    wait()
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 1)
                end
            end
        end)
    end
end})



Window:AddToggle({text = "Auto Power", flag = "toggle", state = false, callback = function(c)
    cc = c
    while cc do task.wait(.5)
        local args = {
            [1] = 1
        }
        
        game:GetService("ReplicatedStorage").RemoteEvents.SkillAttack:FireServer(unpack(args))


local args = {
    [1] = 2
}

game:GetService("ReplicatedStorage").RemoteEvents.SkillAttack:FireServer(unpack(args))

local args = {
    [1] = 3
}

game:GetService("ReplicatedStorage").RemoteEvents.SkillAttack:FireServer(unpack(args))

    end
end})


Window:AddFolder"Credits"

Window:AddLabel({text = "Made by Borntodie1 on v3rm"})


Library:Init()



